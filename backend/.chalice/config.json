{
    "version": "2.0",
    "app_name": "syuking",
    "stages": {
        "dev": {
            "api_gateway_stage": "api",
            "autogen_policy": false,
            "environment_variables": {
                "ENVIRONMENT": "dev",
                "DATABASE_URL": "${DATABASE_URL}",
                "GEMINI_API_KEY": "${GEMINI_API_KEY}",
                "CORS_ALLOW_ORIGIN": "http://localhost:5173"
            }
        },
        "prod": {
            "api_gateway_stage": "api",
            "autogen_policy": false,
            "environment_variables": {
                "ENVIRONMENT": "prod",
                "DATABASE_URL": "${DATABASE_URL}",
                "GEMINI_API_KEY": "${GEMINI_API_KEY}",
                "CORS_ALLOW_ORIGIN": "https://syuking.com"
            }
        }
    }
}